local function KSKJC_fake_script() -- TextButton_5.LocalScript 
	local script = Instance.new('LocalScript', TextButton_5)

	local opene = script.Parent.Parent.Parent.open
	local button = script.Parent
	
	button.MouseButton1Click:Connect(function()
		opene.Visible = true
		script.Parent.Parent.Visible = false
	end)
end
coroutine.wrap(KSKJC_fake_script)()

-- Scripts:

local function IHID_fake_script() -- TextButton.LocalScript 
	local script = Instance.new('LocalScript', TextButton)

	local buton = script.Parent
	local player = game.Players.LocalPlayer
	local text = script.Parent.Parent.floored
	
	buton.MouseButton1Click:Connect(function()
		local character = player.Character or player.CharacterAdded:Wait()
		local root = character:WaitForChild("HumanoidRootPart")
		local pos = root.Position
	
		text.Text = "X: "..math.floor(pos.X).." Y: "..math.floor(pos.Y).." Z: "..math.floor(pos.Z)
	end)
	
end
coroutine.wrap(IHID_fake_script)()
local function DSNY_fake_script() -- Frame.LocalScript 
	local script = Instance.new('LocalScript', Frame)

	local UIS = game:GetService('UserInputService')
	local frame = script.Parent
	local dragToggle = nil
	local dragSpeed = 0.25
	local dragStart = nil
	local startPos = nil
	
	local function updateInput(input)
		local delta = input.Position - dragStart
		local position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X,
			startPos.Y.Scale, startPos.Y.Offset + delta.Y)
		game:GetService('TweenService'):Create(frame, TweenInfo.new(dragSpeed), {Position = position}):Play()
	end
	
	frame.InputBegan:Connect(function(input)
		if (input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch) then 
			dragToggle = true
			dragStart = input.Position
			startPos = frame.Position
			input.Changed:Connect(function()
				if input.UserInputState == Enum.UserInputState.End then
					dragToggle = false
				end
			end)
		end
	end)
	
	UIS.InputChanged:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
			if dragToggle then
				updateInput(input)
			end
		end
	end)
	
end
coroutine.wrap(DSNY_fake_script)()
local function WPBDAL_fake_script() -- TextButton_2.LocalScript 
	local script = Instance.new('LocalScript', TextButton_2)

	local buton = script.Parent
	local player = game.Players.LocalPlayer
	local mouse = player:GetMouse()
	local text = script.Parent.Parent.floored
	local ac = false
	local def
	
	buton.MouseButton1Click:Connect(function()
		ac = not ac
		if ac then
			buton.Text = "ACTIVE"
	
			def = mouse.Button1Down:Connect(function()
				local pos = mouse.Hit.Position
				text.Text = "X: " .. math.floor(pos.X) .. " Y: " .. math.floor(pos.Y) .. " Z: " .. math.floor(pos.Z)
			end)
		else
			buton.Text = "DISABLED"
			wait(1)
			buton.Text = "Clicked Position"
	
			if def then
				def:Disconnect()
				def = nil
			end
		end
	end)
	
end
coroutine.wrap(WPBDAL_fake_script)()
local function LHNETGX_fake_script() -- TextButton_3.LocalScript 
	local script = Instance.new('LocalScript', TextButton_3)

	local buton = script.Parent
	local player = game.Players.LocalPlayer
	local mouse = player:GetMouse()
	local text = script.Parent.Parent.exact
	local ac = false
	local def
	
	buton.MouseButton1Click:Connect(function()
		ac = not ac
		if ac then
			buton.Text = "ACTIVE"
	
			def = mouse.Button1Down:Connect(function()
				local pos = mouse.Hit.Position
				text.Text = "X: " .. (pos.X) .. " Y: " .. (pos.Y) .. " Z: " .. (pos.Z)
			end)
		else
			buton.Text = "DISABLED"
			wait(1)
			buton.Text = "Clicked Position"
	
			if def then
				def:Disconnect()
				def = nil
			end
		end
	end)
	
end
